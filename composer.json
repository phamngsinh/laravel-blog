{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "*",
		"laracasts/flash": "~1.3",
		"illuminate/html": "~5.0",
		"prettus/l5-repository": "~2.0",
		 "apache/log4php": "*",
		"cviebrock/eloquent-sluggable": ">=3.0.0-alpha",
		"sebastian/phpcpd": "*",
		"barryvdh/laravel-ide-helper": "~2.0",
        "xaoc/laravel-js-localization": "dev-master",
		"phaza/laravel-postgis": "^2.13"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"Gis\\": "app/"
		},
	    "files": [
	        "app/Helpers/Common.php",
	         "app/Helpers/Messages.php",
	        "app/Helpers/Cryptor.php"
	    ]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
